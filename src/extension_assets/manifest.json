{
  "manifest_version": 2,

  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "1.0",

  "browser_action": {
    "default_icon": "eye-logo.png",
    "default_popup": "index.html"
